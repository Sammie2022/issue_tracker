<h1>Issue Report</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Status</th>
    <th>Priority</th>
    <th>Project</th>
  </tr>
  <% @issues.each do |issue| %>
    <tr>
      <td><%= issue.title %></td>
      <td><%= issue.status %></td>
      <td><%= issue.priority %></td>
      <td><%= issue.project.title %></td>
    </tr>
  <% end %>
</table>
